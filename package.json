    "@storybook/addon-interactions": "7.0.0-rc.9",
    "@storybook/addon-links": "7.0.0-rc.9",
    "@storybook/blocks": "7.0.0-rc.9",
    "@storybook/cli": "^7.0.0-rc.9",
    "@storybook/preset-create-react-app": "^7.0.0-rc.9",
    "@storybook/react": "^7.0.0-rc.9",
    "@storybook/react-webpack5": "7.0.0-rc.9",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/react": "14.0.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-testing-library": "5.10.2",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.0-rc.9",
    "typescript": "^5.0.2",
    "vitest": "0.29.8",
    "vitest-dom": "0.0.4",
    "webpack": "^5.76.3"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.0.0",
    "axios": "^1.2.0",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-scripts": "^5.0.1",
    "zod": "^3.21.4"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "src/**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
