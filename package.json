    "format": "prettier --write . --ignore-unknown",
    "format-check": "prettier --check . --ignore-unknown",
    "start": "react-scripts start",
    "vitest": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
